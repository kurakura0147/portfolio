= render 'shared/header'
= render 'shared/about'

= form_for @contents do |f|
  =f.label :title
  =f.text_field :name
  =f.label :text
  =f.text_field :text
  =f.label :tag_list, "tag (コンマ区切りです)"
  =f.text_field :tag_list, parse: true
  =f.label :image
  =f.file_field :image
  =f.submit "OK"

= render 'shared/footer'
